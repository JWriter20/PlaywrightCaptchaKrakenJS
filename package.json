    "url": "https://github.com/JWriter20/PlaywrightCaptchaKrakenJS/issues"
  },
  "homepage": "https://github.com/JWriter20/PlaywrightCaptchaKrakenJS#readme",
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^24.10.2",
    "dotenv": "^17.2.3",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "camoufox-js": "^0.8.4",
    "cursory-ts": "^1.0.0",
    "patchright": "^1.57.0",
    "patchright-core": "^1.57.0"
