{
  "name": "playwright-captcha-kraken-js",
  "version": "1.0.2",
  "description": "Playwright wrapper which uses CaptchaKraken-cli to solve captchas.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
